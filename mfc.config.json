{
  "TARGET": "libcryptography.so.5.0.0",
  "CC": "g++",
  "LD": "ld",
  "C_FLAGS": [
    "-c",
    "-Wall",
    "-fPIC"
  ],
  "INCLUDE_PATHS": [
    "include"
  ],
  "LIBS": [
    "crypto"
  ],
  "LD_FLAGS": [
    "-soname libcryptography.so.5",
    "-shared"
  ],
  "EXTENSIONS": [
    "cc"
  ],
  "IGNORE_PATHS": [
    ".git",
    ".vscode",
    "tests",
    "build"
  ],
  "CUSTOM_TARGETS": {
    "test": "cd ../tests && make && ./tests"
  },
  "VERBOSE": false,
  "RM": "rm -v",
  "BUILD_DIRECTORY": "./build",
  "BIN_DIRECTORY": "./build"
}
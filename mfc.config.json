{
  "TARGET": "libcryptography.so.6.0.0",
  "CC": "g++",
  "LD": "ld",
  "C_FLAGS": [
    "-c",
    "-Wall",
    "-fPIC",
    "-g",
    "-O0"
  ],
  "LD_FLAGS": [
    "-shared",
    "-soname libcryptography.so.6"
  ],
  "LIBS": [
    "crypto"
  ],
  "INCLUDE_PATHS": [
  ],
  "EXTENSIONS": [
    "cc"
  ],
  "IGNORE_PATHS": [
    ".vscode",
    "cryptutil",
    "deb",
    "tests"
  ],
  "CUSTOM_TARGETS": {
    "static": "ar -rvs libcryptography.a ./src/*.o"
  },
  "VERBOSE": false,
  "RM": "rm -v",
  "BUILD_DIRECTORY": "./build",
  "BIN_DIRECTORY": "./build"
}